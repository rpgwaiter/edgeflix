{
  "name": "edgeflix-tracker",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx -y esbuild ./src/worker.ts --bundle --outdir='dist' --platform=browser --format=esm",
    "deploy": "wrangler --config wrangler-${CI_COMMIT_REF_NAME-$(git rev-parse --symbolic-full-name --abbrev-ref HEAD)}.toml publish",
    "start": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' node ./dev.js",
    "test": "npx -y vitest --run --reporter verbose"
  },
  "devDependencies": {
    "create-torrent": "^5.0.9",
    "cross-cf": "^0.0.10",
    "dotenv": "^16.0.3",
    "miniflare": "^2.11.0",
    "parse-torrent": "^9.1.5",
    "vitest": "^0.25.3",
    "webtorrent": "^1.9.4",
    "wrangler": "^2.1.13"
  },
  "dependencies": {
    "bencode": "^2.0.3"
  }
}
