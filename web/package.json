  },
  "devDependencies": {
    "@astrojs/cloudflare": "^4.0.1",
    "@astrojs/svelte": "^1.0.2",
    "astro": "^1.6.10",
    "buffer": "^6.0.3",
    "svelte": "^3.53.1",
    "wrangler": "^2.4.4"
  }
}
